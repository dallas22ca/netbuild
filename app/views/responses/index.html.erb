<%= render "manage_nav" %>

<h1>Responses</h1>

<table class="table">
  <thead>
    <tr>
      <th>Block</th>
      <th>Data</th>
      <th>Contact</th>
      <th>Website</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @responses.each do |response| %>
      <tr>
        <td><%= response.block %></td>
        <td><%= response.data %></td>
        <td><%= response.contact %></td>
        <td><%= response.website %></td>
        <td><%= link_to 'Show', response %></td>
        <td><%= link_to 'Edit', edit_response_path(response) %></td>
        <td><%= link_to 'Destroy', response, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>