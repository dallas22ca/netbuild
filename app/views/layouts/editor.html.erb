<!DOCTYPE html>
<html>
<head>
  <title><%= @page.title %> - <%= @website.title %></title>
  <%= stylesheet_link_tag    "editor", theme_path(@website.theme, format: :css), media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "editor", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body data-page_id="<%= @page.id if @page %>">
	
<%= render "pages/includes/editor" %>

<div id="timetravel">
	<%= render "pages/includes/yield" %>
</div>

</body>
</html>